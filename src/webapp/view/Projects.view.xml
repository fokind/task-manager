<View controllerName="fokind.kanban.controller.Projects" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" displayBlock="true">
	<Page title="Projects">
		<headerContent>
			<Button icon="sap-icon://add" press="onAddPress" />
		</headerContent>
		<List items="{/Projects}" id="projects">
			<StandardListItem title="{title}" press="onItemPress" type="Navigation" />
		</List>
	</Page>
</View>
