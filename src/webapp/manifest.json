{
    "sap.app": {
        "id": "fokind.kanban",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "default": {
                "uri": "/odata/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui5": {
        "dependencies": {
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "": {
                "dataSource": "default",
                "settings": {
                    "autoExpandSelect": false,
                    "operationMode": "Server",
                    "synchronizationMode": "None",
                    "groupId": "$direct"
                },
                "type": "sap.ui.model.odata.v4.ODataModel"
            },
			"draft": {
				"type": "sap.ui.model.json.JSONModel"
			}
        },
        "rootView": {
            "viewName": "fokind.kanban.view.App",
            "type": "XML"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "fokind.kanban.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "projects",
                    "target": "projects"
                },
                {
                    "pattern": "project/{id}/edit",
                    "name": "projectEdit",
                    "target": "projectEdit"
                },
                {
                    "pattern": "project/{id}/kanban",
                    "name": "projectKanban",
                    "target": "projectKanban"
                }
            ],
            "targets": {
                "projects": {
                    "viewName": "Projects",
                    "viewLevel": 1
                },
                "projectEdit": {
                    "viewName": "ProjectEdit",
                    "viewLevel": 2
                },
                "projectKanban": {
                    "viewName": "ProjectKanban",
                    "viewLevel": 2
                }
            }
        }
    }
}
