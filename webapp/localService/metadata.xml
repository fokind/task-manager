<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx">
	<edmx:DataServices m:DataServiceVersion="2.0" m:MaxDataServiceVersion="4.0"
					   xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
		<Schema Namespace="Kanban" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
			<EntityType Name="Project">
				<Key>
					<PropertyRef Name="id"/>
				</Key>
				<Property Name="id" Type="Edm.String" Nullable="false"/>
				<Property Name="title" Type="Edm.String" Nullable="true"/>
				<NavigationProperty Name="ToDo"
				    Relationship="Kanban.ProjectToDo"
				    FromRole="FromRole_ProjectToDo"
				    ToRole="ToRole_ProjectToDo"/>
				<!--<NavigationProperty Name="InProgress"-->
				<!--    Relationship="Kanban.ProjectInProgress"-->
				<!--    FromRole="FromRole_ProjectInProgress"-->
				<!--    ToRole="ToRole_ProjectInProgress"/>-->
				<!--<NavigationProperty Name="Done"-->
				<!--    Relationship="Kanban.ProjectDone"-->
				<!--    FromRole="FromRole_ProjectDone"-->
				<!--    ToRole="ToRole_ProjectDone"/>-->
			</EntityType>
			<EntityType Name="Task">
				<Key>
					<PropertyRef Name="id"/>
				</Key>
				<Property Name="id" Type="Edm.String" Nullable="false"/>
				<Property Name="state" Type="Edm.String" Nullable="false"/>
				<Property Name="projectId" Type="Edm.String" Nullable="false"/>
				<Property Name="title" Type="Edm.String" Nullable="false"/>
			</EntityType>
			<Association Name="ProjectToDo">
			    <End Type="Kanban.Project" Multiplicity="1" Role="FromRole_ProjectToDo"/>
			    <End Type="Kanban.Task" Multiplicity="*" Role="ToRole_ProjectToDo"/>
			</Association>
			<!--<Association Name="ProjectInProgress">-->
			<!--    <End Type="Kanban.Project" Multiplicity="1" Role="FromRole_ProjectInProgress"/>-->
			<!--    <End Type="Kanban.Task" Multiplicity="*" Role="ToRole_ProjectInProgress"/>-->
			<!--</Association>-->
			<!--<Association Name="ProjectDone">-->
			<!--    <End Type="Kanban.Project" Multiplicity="1" Role="FromRole_ProjectDone"/>-->
			<!--    <End Type="Kanban.Task" Multiplicity="*" Role="ToRole_ProjectDone"/>-->
			<!--</Association>-->
		</Schema>
		<Schema Namespace="OData" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
			<EntityContainer Name="Entities" m:IsDefaultEntityContainer="true">
				<EntitySet Name="Projects" EntityType="Kanban.Project"/>
				<EntitySet Name="Tasks" EntityType="Kanban.Task"/>
				<AssociationSet Name="ProjectToDo" Association="Kanban.ProjectToDo">
    			    <End EntitySet="Projects" Role="FromRole_ProjectToDo"/>
    			    <End EntitySet="Tasks" Role="ToRole_ProjectToDo"/>
				</AssociationSet>
				<!--<AssociationSet Name="ProjectInProgress" Association="Kanban.ProjectInProgress">-->
    <!--			    <End EntitySet="Projects" Role="FromRole_ProjectInProgress"/>-->
    <!--			    <End EntitySet="Tasks" Role="ToRole_ProjectInProgress"/>-->
				<!--</AssociationSet>-->
				<!--<AssociationSet Name="ProjectDone" Association="Kanban.ProjectDone">-->
    <!--			    <End EntitySet="Projects" Role="FromRole_ProjectDone"/>-->
    <!--			    <End EntitySet="Tasks" Role="ToRole_ProjectDone"/>-->
				<!--</AssociationSet>-->
			</EntityContainer>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>